{
  "private": true,
  "main": "index.js",
  "version": "0.0.1",
  "scripts": {
    "start:native": "node node_modules/react-native/local-cli/cli.js",
    "start:android": "npm run start:native -- run-android",
    "start:ios": "npm run start:native -- run-ios",
    "start:windows": "npm run start:native -- run-windows",
    "start:web": "cross-env RX_PLATFORM=web webpack-dev-server --config=web/webpack/dev.js --progress --colors --mode=development",
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "build:web": "cross-env RX_PLATFORM=web webpack --config=web/webpack/prod.js --progress --colors --mode=production",
    "lint": "esw src/**"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.1.2",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "cross-env": "5.2.0",
    "eslint": "5.6.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "html-webpack-plugin": "3.2.0",
    "metro-react-native-babel-preset": "0.48.1",
    "rnpm-plugin-windows": "0.2.8",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9",
    "webpack-merge": "4.1.4"
  }
}
